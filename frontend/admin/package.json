{
  "name": "mhgoi_admin",
  "version": "1.1.0",
  "description": "mhgoi_admin",
  "private": true,
  "author": "dyrnq@qq.com",
  "scripts": {
    "dev": "vue-cli-service serve --host 0.0.0.0 --port 9999",
    "pic": "ecstatic --cors --baseDir pic --showDir true --autoIndex true --host 0.0.0.0 --port 8006 --root /data/pic",
    "build": "vue-cli-service build",
    "build:prod": "vue-cli-service build",
    "build:stage": "vue-cli-service build --mode staging",
    "preview": "node build/index.js --preview",
    "svgo": "svgo -f src/icons/svg --config=src/icons/svgo.yml",
    "lint": "eslint --ext .js,.vue src",
    "test:unit": "jest --clearCache && vue-cli-service test:unit",
    "test:ci": "npm run lint && npm run test:unit",
    "dotenv": "dotenv -c development"
  },
  "dependencies": {
    "axios": "^1.1.3",
    "core-js": "^3.6.5",
    "element-ui": "^2.15.12",
    "js-cookie": "^3.0.1",
    "mavon-editor": "^2.10.4",
    "normalize.css": "8.0.1",
    "nprogress": "0.2.0",
    "path-to-regexp": "6.2.1",
    "vue": "^2.7.14",
    "vue-axios": "^3.5.2",
    "vue-router": "3.5.2",
    "vuex": "3.6.2",
    "el-input-tag": "^1.3.3",
    "path-browserify": "^1.0.1",
    "vue-json-viewer": "^2.2.22"
  },
  "devDependencies": {
    "@georges-tech/tasksfile": "5.2.0",
    "@vue/cli-plugin-babel": "4.4.4",
    "@vue/cli-plugin-eslint": "4.4.4",
    "@vue/cli-plugin-unit-jest": "4.4.4",
    "@vue/cli-service": "4.4.4",
    "@vue/test-utils": "1.3.3",
    "autoprefixer": "9.8.8",
    "babel-eslint": "10.1.0",
    "babel-jest": "29.7.0",
    "babel-plugin-dynamic-import-node": "2.3.3",
    "chalk": "5.1.2",
    "connect": "3.7.0",
    "dotenv-cli": "^4.0.0",
    "ecstatic": "^4.1.4",
    "eslint": "^8.27.0",
    "eslint-plugin-vue": "^9.7.0",
    "html-webpack-plugin": "4.0.0",
    "less-loader": "^6.2.0",
    "mockjs": "1.0.1-beta3",
    "sass": "1.26.8",
    "sass-loader": "8.0.2",
    "script-ext-html-webpack-plugin": "2.1.5",
    "svgo": "3.0.2",
    "tasksfile": "^5.1.1",
    "serve-static": "1.15.0",
    "vue-template-compiler": "2.7.14"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "engines": {
    "node": ">=8.9",
    "npm": ">= 3.0.0"
